{
  "manifest_version": 3,
  "name": "Urgency Randomizer",
  "description": "Un'estensione scherzosa che sostituisce la parola urgente e le sue varianti nella pagina.",
  "version": "0.1.4",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}